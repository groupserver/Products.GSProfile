<tal:block 
  define="userInfo options/view/userInfo;
    uri string:/s/search.ajax?a=${userInfo/id};
    postsUri string:&quot;${uri}&amp;t=0&amp;p=1&amp;f=0&amp;r=0&quot;;
    filesUri string:&quot;${uri}&amp;t=0&amp;p=0&amp;f=1&amp;r=0&quot;">
        jQuery.noConflict()
        jQuery(document).ready( function () {
            var gsRequiredWidgets = new Array(<span tal:replace="arr" />);
            GSCheckRequired.init(
              gsRequiredWidgets, 
              '#form\\.actions\\.edit');
              
            GSPopupFormHelp.init('#edit-profile');

            jQuery("#privacy-button").click( function () {
                var gsUri = "/policies/privacy/ #privacy";
                jQuery("#privacy-content").load(gsUri);
            });
        });
</tal:block>

